{
  "name": "@yuiblog/functions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf lib/ && tsc",
    "dev": "npm run build && firebase serve --only hosting,functions",
    "dev:express": "NODE_ENV=development nodemon --watch ./src --ext .ts --exec ts-node src/dev.ts",
    "start": "npm run shell",
    "deploy": "node bin/deploy.js"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.7.8",
    "express": "^4.16.4",
    "firebase-admin": "^6.4.0",
    "firebase-functions": "^2.1.0",
    "lodash": "^4.17.11",
    "qs": "^6.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/lodash": "^4.14.118",
    "@types/qs": "^6.5.1",
    "@yuiblog/types": "^0.1.0",
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.9",
    "ts-node": "^8.0.1",
    "tslint": "~5.8.0",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": "8"
  }
}
