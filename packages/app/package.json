{
  "name": "@yuiblog/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nuxt build && tsc --project ./functions/tsconfig.json",
    "postbuild": "yarn run copy",
    "precopy": "rm -rf dist && mkdir dist",
    "copy": "yarn run copy:client && yarn run copy:server",
    "copy:client": "cp -R ./static/. dist/client && cp -R .nuxt/dist/client dist/client/assets",
    "copy:server": "cp -R ./functions/ dist/server && cp -R .nuxt/ dist/server/lib && cp package.json dist/server && cp yarn.lock dist/server",
    "dev": "nuxt",
    "start": "nuxt start",
    "deploy": "firebase deploy --only functions:renderNuxt,hosting:app"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@nuxtjs/dotenv": "1.3.0",
    "@nuxtjs/google-analytics": "2.0.2",
    "@nuxtjs/pwa": "2.6.0",
    "@yuiblog/markdown": "0.1.5",
    "axios": "0.19.0",
    "dayjs": "1.8.12",
    "express": "4.16.4",
    "firebase-admin": "6.5.0",
    "firebase-functions": "2.1.0",
    "marked": "0.5.2",
    "nuxt": "2.3.4",
    "nuxt-class-component": "1.2.1",
    "nuxt-property-decorator": "1.3.1",
    "prismjs": "1.15.0",
    "qs": "6.7.0",
    "vuex-class": "0.3.1",
    "vuex-type-helper": "1.2.0"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/marked": "0.4.2",
    "@types/prismjs": "1.9.0",
    "@types/qs": "6.5.2",
    "@yuiblog/types": "^0.1.0",
    "autoprefixer": "8.6.5",
    "cross-env": "5.2.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-prettier": "2.7.0",
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "pug": "3.0.1",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "7.1.0",
    "tailwindcss": "0.6.6",
    "ts-loader": "5.3.3",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.4.2"
  },
  "engines": {
    "node": "8"
  }
}