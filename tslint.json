{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "prettier": true
  }
}