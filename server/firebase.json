{
  "functions": {
    "predeploy": [],
    "source": "./",
    "ignore": [
      ".*",
      "*.log",
      "tsconfig.json",
      "**/src/**",
      "**/node_modules/**"
    ]
  },
  "hosting": {
    "target": "api",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "api"
      }
    ]
  }
}